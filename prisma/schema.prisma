generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Property {
  id           String   @id @default(uuid())
  title        String
  price        Float
  type         String   // ex: "Apartamento", "Casa"
  intent       String   // ex: "Venda", "Aluguel"
  city         String
  bedrooms     Int
  area         Float
  imageUrl     String?
  createdAt    DateTime @default(now())
}